        <servlet-name>MakeFoodEntry</servlet-name>
        <url-pattern>/MakeFoodEntry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowFoodDetails1</servlet-name>
        <url-pattern>/ShowFoodDetails1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
