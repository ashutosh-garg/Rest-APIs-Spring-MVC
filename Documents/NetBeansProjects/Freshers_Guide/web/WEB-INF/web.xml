    <servlet>
        <servlet-name>ShowFoodDetails1</servlet-name>
        <servlet-class>ShowFoodDetails1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserValidation</servlet-name>
        <url-pattern>/UserValidation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeServiceEntry</servlet-name>
        <url-pattern>/MakeServiceEntry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeEntry</servlet-name>
        <url-pattern>/MakeEntry</url-pattern>
    </servlet-mapping>
